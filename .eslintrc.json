{
  "env": {
    "commonjs": true,
    "es2020": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "no-console": "off",
    "comma-dangle": "off"
  }
}
